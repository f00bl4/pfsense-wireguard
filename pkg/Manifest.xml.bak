<package>
      <name>Wireguard</name>
      <internal_name>Wireguard</internal_name>
      <pkginfolink>https://github.com/f00bl4/pfsense-wireguard</pkginfolink>
      <descr><![CDATA[Wireguard Plugin for pfSense]]></descr>
      <website>https://github.com/f00bl4/pfsense-wireguard</website>
      <category>VPN</category>
      <version>0.5</version>
      <status>Beta</status>
      <required_version>2.2</required_version>
      <config_file>https://packages.pfsense.org/packages/config/someprogram/someprogram.xml</config_file>
      <maintainer>me@example.com</maintainer>
      <configurationfile>someprogram.xml</configurationfile>
      <only_for_archs>i386 amd64</only_for_archs>
</package>


<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE packagegui SYSTEM "../schema/packages.dtd">
<?xml-stylesheet type="text/xsl" href="../xsl/package.xsl"?>
