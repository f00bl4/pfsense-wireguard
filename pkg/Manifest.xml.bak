	<name>wireguard</name>
	<title>Wireguard</title>
	<aftersaveredirect>/pkg_edit.php?xml=haproxy_pools.php</aftersaveredirect>
	<include_file>/usr/local/pkg/haproxy/haproxy.inc</include_file>        /*was ist das???*/
	<menu>
		<name>Wireguard</name>
		<tooltiptext></tooltiptext>
		<section>VPN</section>
		<url>/wireguard/wg_config.php</url>
	</menu>
	<menu>
		<name>Wireguard Stats</name>
		<tooltiptext>Stats of Wireguard</tooltiptext>
		<section>Status</section>
		<url>/wireguard/wg_stats.php</url>
	</menu>
	<service>                                                               /*service und plugins n√∂tig?*/
		<name>haproxy</name>
		<rcfile>haproxy.sh</rcfile>
		<executable>haproxy</executable>
		<description>TCP/HTTP(S) Load Balancer</description>
	</service>	
	<plugins>
		<item>
			<type>plugin_carp</type>
		</item>
		<item>
			<type>plugin_certificates</type>
		</item>
	</plugins>
	<configpath>installedpackages->wireguard->config</configpath>
	<custom_php_install_command>
		wireguard_custom_php_install_command();
	</custom_php_install_command>
	<custom_php_deinstall_command>
		wireguard_custom_php_deinstall_command();
	</custom_php_deinstall_command>
	<filter_rules_needed>
		wireguard_generate_rules
	</filter_rules_needed>
</packagegui>